#using FacebookToInstagramIntegration.*
#using AttrCondDefLibrary.*

#rule Bridge #with FacebookToInstagramIntegration

#source { 
	f:FacebookLanguage.Facebook {
    ++ -users->fu
  }
   fu:FacebookLanguage.User
}

#target {
	i:InstagramLanguage.Instagram {
    ++ -users->iu
  }
   iu:InstagramLanguage.User
	
}

#correspondence {
	f2i:NetworkToNetwork{
    #src->f
    #trg->i
  }
    fu2iu:FbUserToInUser{
    #src->fu
    #trg->iu
  }
}

#attributeConditions {
	
}
