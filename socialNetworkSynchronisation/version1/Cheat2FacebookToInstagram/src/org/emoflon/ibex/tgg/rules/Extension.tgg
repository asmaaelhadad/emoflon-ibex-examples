#using Cheat2FacebookToInstagram.*
#using AttrCondDefLibrary.*

#rule Extension #with Cheat2FacebookToInstagram

#source { 
	fl:Facebook {
		++ -users->flu
	}
	++ flu:FacebookLanguage.User
}

#target {
	il:Instagram {
		++ -users->ilu
	}
	++ ilu:InstagramLanguage.User
}

#correspondence {
	n2n:NetworkToNetwork {
		#src->fl
		#trg->il
	}
	++ f2i:FbUserToInUser {
		#src-> flu
		#trg-> ilu
	}
	
}

#attributeConditions {
	eq_string(flu.name,ilu.name)
}
